// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model YoutubeVideo {
  id      String @id @unique
  title   String
  author  String
  pubDate String
  link    String
}

model YoutubeChannelSubscription {
  id              String @id @unique
  outputChannelId String
}
